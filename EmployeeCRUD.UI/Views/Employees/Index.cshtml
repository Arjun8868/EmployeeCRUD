@model IEnumerable<EmployeeCRUD.UI.Models.DTO.EmployeeDTO>
<h1 class="mt-3">Employees</h1>

<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
           
            <th>Email</th>
            <th>Phone Number</th>
            <th>Salary</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach(var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                
                <td>@employee.Email</td>
                <td>@employee.Phone</td>
                <td>@employee.Salary</td>
                
            </tr>
        }
    </tbody>
</table>



